    "migration:run": "npm run build-backend && npm run typeorm -- migration:run",
    "nx": "nx",
    "serve": "nx run-many --target=serve --all --parallel=true",
    "serve:backend": "nx serve backend",
    "serve:frontend": "nx serve frontend",
    "serve:thumbnailer": "nx serve thumbnailer",
    "start-backend": "node dist/apps/backend/apps/backend/src/main.js",
    "start-dist": "npm run build-all && node -r dotenv/config ./dist/apps/backend/main.js",
    "start:dev": "ts-node -P apps/backend/tsconfig.app.json apps/backend/src/main.ts",
    "start": "nx run-many --target=serve --projects=frontend,thumbnailer --parallel=true",
    "test-all": "nx run-many --target=test --all",
    "test-backend": "jest -c apps/backend/jest.config.js",
    "test": "nx test",
    "typeorm": "npx typeorm -d dist/apps/backend/db/data-sources.js",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator"
  },
