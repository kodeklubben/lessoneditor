  "scripts": {
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "affected": "nx affected",
    "build:all": "nx run-many --target=build --all",
    "build:backend": "tsc --p apps/backend/tsconfig.app.json",
    "build:dev": "concurrently \"npm run build:all\" \"npm run build:backend\" \"npm run build:thumbnailer:dev\"",
    "build:thumbnailer": "tsc --p apps/thumbnailer/tsconfig.app.json",
    "build": "(tsc --p apps/backend/tsconfig.app.json) && (nx build)",
    "dep-graph": "nx dep-graph",
    "dev:backend": "node dist/apps/backend/apps/backend/src/main.js",
    "dev:frontend": "nx serve frontend",
    "dev:thumbnailer": "functions-framework --source=dist/out-tsc/apps/thumbnailer/src --target=thumbnailer --port=3012",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "format": "nx format:write",
    "help": "nx help",
    "lint": "nx workspace-lint && nx lint",
    "migration:generate": "npm run typeorm -- migration:generate",
    "migration:revert": "npm run typeorm -- migration:revert",
    "migration:run": "npm run build:backend && npm run typeorm -- migration:run",
    "nx": "nx",
    "serve:backend": "nx serve backend",
    "serve:frontend": "nx serve frontend",
    "serve:thumbnailer": "nx serve thumbnailer",
    "serve": "nx run-many --target=serve --all --parallel=true",
    "start": "nx run-many --target=serve --projects=frontend,thumbnailer --parallel=true",
    "start:dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\" \"npm run dev:thumbnailer\"",
    "test-all": "nx run-many --target=test --all",
    "test-backend": "jest -c apps/backend/jest.config.js",
    "test": "nx test",
    "typeorm": "npx typeorm -d dist/apps/backend/db/data-sources.js",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator"
  },
  "private": true,
