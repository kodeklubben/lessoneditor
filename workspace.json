{
  "version": 2,
  "projects": {
    "api-interfaces": {
      "root": "libs/api-interfaces",
      "sourceRoot": "libs/api-interfaces/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/api-interfaces/**/*.ts"]
          }
        }
      },
      "tags": []
    },
    "contracts": {
      "root": "libs/contracts",
      "sourceRoot": "libs/contracts/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/contracts/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/contracts"],
          "options": {
            "jestConfig": "libs/contracts/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/contracts",
            "tsConfig": "libs/contracts/tsconfig.lib.json",
            "packageJson": "libs/contracts/package.json",
            "main": "libs/contracts/src/index.ts",
            "assets": ["libs/contracts/*.md"]
          }
        }
      },
      "tags": []
    },
    "frontend": "apps/frontend",
    "thumbnailer": {
      "root": "apps/thumbnailer",
      "sourceRoot": "apps/thumbnailer/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "parallel": false,
            "commands": [
              {
                "command": "nx build-app thumbnailer"
              },
              {
                "command": "node ./apps/thumbnailer/build.js"
              }
            ]
          }
        },
        "build-app": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/thumbnailer",
            "main": "apps/thumbnailer/src/function.ts",
            "tsConfig": "apps/thumbnailer/tsconfig.app.json"
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/thumbnailer/src/environments/environment.ts",
                  "with": "apps/thumbnailer/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "buildTarget": "thumbnailer:build",
            "parallel": false,
            "commands": [
              {
                "command": "nx build-app thumbnailer"
              },
              {
                "command": "functions-framework --target=thumbnailer --port=3012 --source=dist/apps/thumbnailer/main.js"
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/thumbnailer/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/thumbnailer"],
          "options": {
            "jestConfig": "apps/thumbnailer/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    }
  }
}
